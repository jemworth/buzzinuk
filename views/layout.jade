doctype html
html

	head
	meta(charset='utf8')
	meta(http-equiv='X-UA-Compatible', content='IE=edge, chrome=1')
	//- uncomment if your site is responsive!
	meta(name="viewport" content="width=device-width, initial-scale=1")

	meta(name='description', content= records.site.sitedata[0].metadesc)
	meta(name='author', content= records.site.sitedata[0].metauthor)
	meta(name="keywords" content= records.site.sitedata[0].metakeyword)

	title= records.site.sitedata[0].sitename

	link(rel="icon", href= records.site.sitedata[0].icon, type="image/*")
	link(rel="shortcut icon", href= records.site.sitedata[0].icon, type="img/*")
	link(href='https://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700,800', rel='stylesheet', type='text/css')
	link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css')
	link(rel='stylesheet', href='../css/webriq-bootstrap-material-ui.css')
	link(rel='stylesheet', href='../css/custom.css')
	link(rel='stylesheet', href='../css/master.css')
	//- link(rel='stylesheet', href='../css/demo-web.css')
	//- link(rel='stylesheet', href='../css/normalize-web.css')
	link(rel='stylesheet', href='../css/preload-main.css')
	script(type="text/javascript", src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.2.4/jquery.min.js")
	<!-- add this below material-ui.css -->
	link(rel='stylesheet', href='//cdn.materialdesignicons.com/1.7.22/css/materialdesignicons.min.css')
	<!-- Custom CSS -->

	body#page-top

	.main-wrapper#page

		.header-menu
			include includes/menu
		.head-bg
			include includes/header

		div.main-container
			block content

		footer
			include includes/footer

	#loading
